<h2>Available drones</h2>

{{#each drones}}
<div>
    <h1>Name: {{name}}</h1>
    <p>Propellers: {{propellers}}</p>
    <p>Maximum Speed: {{maxSpeed}}</p>

    <a href="/drones/{{_id}}/edit">Edit Drone</a>
    <form action="/drones/{{_id}}/delete" method="POST">

    <button type="submit"><a href="/drones/{{this._id}}/delete">Delete</a></button>
    </form>
</div>
{{/each}} 
